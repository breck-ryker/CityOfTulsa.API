
@model CityOfTulsaUI.Models.UserModel

@{
   CityOfTulsaUI.Classes.AppSettings appSettings = this.ViewBag.AppSettings;
   ViewData["Title"] = "CityofTulsaData.com - Web API";
}

@section Styles
{
   <link rel="stylesheet" type="text/css" href="@Url.Content("/css/aboutcotwebapi.css")" />
}

@section Scripts
{
}

<div class="card text-center">
   @*<div class="card-header"></div>*@
   <div class="card-body">
      <h5 class="card-title">@ViewData["Title"]</h5>
      <p class="card-text text-muted">* Some web methods require a Json Web Token or API Key *</p>
      <p class="card-text">Explore the CityofTulsaData.com Web API here:</p>
      <a href="@((appSettings.APISwaggerURL ?? ""))" target="_blank" class="btn btn-primary">Web API</a>
   </div>
   <div class="card-footer text-muted">
      (Not affiliated with <a class="" href="@((appSettings.CityOfTulsaOrgURL ?? ""))" target="_blank"><u>City of Tulsa</u></a>)
   </div>
</div>